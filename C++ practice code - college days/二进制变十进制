#include<iostream>
using namespace std;
int power(int a,int b)
{
	int val=1;
	while(b--) val*=a;
	return val;
}
int main ()
{
	int i,all=0;
	char m;
	cin>>m;
	if(m=='0')
	{
	    for(i=6;i>=0;i--)
	    {
		    char ch;
		    cin>>ch;
		    if(ch=='1')
		        all+=power(2,i) ;	    
	    }
	    cout<<all<<endl;
	}
	else if(m=='1')
	{
		cout<<'-';
		for(i=6;i>=0;i--)
	    {
		    char ch;
		    cin>>ch;
		    if(ch=='1')
		        all+=power(2,i) ;	    
	    }
	    cout<<all<<endl;
	}
	else cout<<"ÊäÈëÓÐÎó£¡"<<endl; 
	return 0;
} 